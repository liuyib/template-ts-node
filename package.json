{
  "name": "ts-node-template",
  "version": "0.0.1",
  "author": "liuyib <https://github.com/liuyib/>",
  "description": "A template repo for TypeScript",
  "keywords": [
    "typescript",
    "template"
  ],
  "scripts": {
    "dev": "webpack --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "build": "webpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
